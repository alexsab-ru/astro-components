---
import scripts_json from '@/data/scripts.json';
const isModeProd = import.meta.env.MODE === 'production';
const isProd = import.meta.env.PROD;
---
{ isProd && scripts_json.konget && scripts_json.konget.value != "" && (
<script>
function runKONGET(konget) {
    // console.log("runKONGET", konget);
    var script = document.createElement('script');
    script.src = `//app.konget.ru/inject?token=${konget}`;
    script.async = true;
    document.head.appendChild(script);
}
window._kg = runKONGET;
</script>
)}
